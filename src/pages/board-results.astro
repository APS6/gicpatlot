---
import '@fontsource/poppins/400.css';
import '@fontsource/poppins/500.css';
import '@fontsource/poppins/600.css';
import '@fontsource/poppins/700.css';
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
---
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" href="/base/favicon.ico" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<meta name="description" content="Check your 2025 CBSE board exam results here." />
		<meta name="keywords" content="Board results, Class 10 results, Class 12 results, GIC Patlot, Patlot, CBSE Results">
		<meta property="og:title" content="Board results | PM Shri Atal Utkrisht SBSM GMIC Patlot">
		<meta property="og:description" content="Check your 2025 CBSE board exam results here. Official website for GIC Patlot.">
		<meta property="og:image" content="https://gicpatlot.in/base/og.jpg">
		<meta property="og:url" content="https://gicpatlot.in/">
		<meta property="twitter:card" content="summary_large_image">
		<meta property="twitter:title" content="Board results | PM Shri Atal Utkrisht SBSM GMIC Patlot">
		<meta property="twitter:description" content="Check your 2025 CBSE board exam results here. Official website for GIC Patlot.">
		<meta property="twitter:image" content="https://gicpatlot.in/base/og.jpg">
		<title>Check Board Results | PM Shri Atal Utkrisht SBSM GIC Patlot</title>
	</head>
	<body class=" bg-bgCol text-text">
		<Navigation />
		<main class="max-w-[120rem] m-auto px-4 md:px-10 2xl:px-12 pt-24 md:pt-28 flex flex-col justify-between">
  <div class="h-[50svh] w-full flex flex-col items-center">
      <h1 class="text-2xl md:text-3xl font-bold text-center">Check your board exam results</h1>
      <div class="flex flex-col gap-6 mt-12 w-full max-w-2xl">
          <a href="https://cbseresults.nic.in/2025/CBSE10th/CBSE10thLogin?resultType=cbse10"><button class="bg-secondary w-full py-4 rounded-xl text-xl">Class 10</button></a>
          <a href="https://cbseresults.nic.in/2025/CBSE12th/CBSE12thLogin?resultType=cbse12"><button class="bg-secondary w-full py-4 rounded-xl text-xl">Class 12</button></a>
      </div>
  </div>
            <Footer />
        </main>
	</body>
</html>
